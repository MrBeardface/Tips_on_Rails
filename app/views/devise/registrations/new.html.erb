<div class="row">
  <div class="col-sm-6 col-sm-offset-3 col-md-4 col-md-offset-4">

    <h3>Sign up</h3>

    <div class="well well-lg">

      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>

        <div class="form-group">
          <%= f.text_field :first_name, class: 'form-control', placeholder: 'Your first name', autofocus: true, value: session["devise.github_data"]["info"]["name"].split(' ')[0] %>
        </div>

        <div class="form-group">
          <%= f.text_field :last_name, class: 'form-control', placeholder: 'Your last name', value: session["devise.github_data"]["info"]["name"].split(' ')[1] %>
        </div>

        <div class="form-group">
          <%= f.text_field :email, class: 'form-control', placeholder: 'Your email address', value: session["devise.github_data"]["info"]["email"] %>
        </div>

        <%= f.button 'Sign up', class: 'btn btn-success pull-right', data: { disable_with: 'Please wait...' } %>

      <% end %>

      <%= render "devise/shared/links" %>

    </div>
    <pre>
        <%= simple_format session["devise.github_data"].to_yaml %>
      </pre>
  </div>
</div>
